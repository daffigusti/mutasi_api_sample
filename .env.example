# Mutasibank Webhook Configuration

# Webhook Secret (64 characters)
# Get this from: Dashboard > Pengaturan > Webhook
WEBHOOK_SECRET=your_64_char_webhook_secret_from_dashboard

# API Token
# Get this from: Dashboard > Developer API
API_TOKEN=your_api_token_from_dashboard

# Timestamp Tolerance (seconds)
# How old can a webhook be before rejecting (default: 5 minutes)
TIMESTAMP_TOLERANCE=300

# Enable Debug Logging
ENABLE_LOGGING=true
